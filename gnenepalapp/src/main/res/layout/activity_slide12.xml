<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/activity_vertical_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                tools:context=".Slide12Activity" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
    <ImageView
        android:id="@+id/foodpic"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:onClick="foodTouch"
        android:src="@drawable/slide_12_13"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox4"
        android:layout_gravity="right"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/foodpic"
        android:layout_alignStart="@+id/foodpic"
        android:visibility="invisible"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox"
        android:layout_gravity="right"
        android:layout_above="@+id/checkBox7"
        android:layout_toRightOf="@+id/checkBox9"
        android:onClick="cb1"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox6"
        android:layout_gravity="right"
        android:layout_marginRight="37dp"
        android:layout_below="@+id/checkBox2"
        android:layout_alignRight="@+id/foodpic"
        android:layout_alignEnd="@+id/foodpic"
        android:onClick="cb2"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox9"
        android:layout_gravity="right"
        android:layout_alignTop="@+id/checkBox7"
        android:layout_centerHorizontal="true"
        android:onClick="cb3"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox2"
        android:layout_gravity="right"
        android:layout_below="@+id/checkBox5"
        android:layout_toLeftOf="@+id/checkBox6"
        android:onClick="cb4"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox10"
        android:layout_gravity="right"
        android:layout_marginLeft="27dp"
        android:layout_alignTop="@+id/checkBox8"
        android:layout_alignLeft="@+id/checkBox7"
        android:layout_alignStart="@+id/checkBox7"
        android:onClick="cb5"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox8"
        android:layout_gravity="right"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/checkBox6"
        android:layout_toLeftOf="@+id/checkBox6"
        android:onClick="cb6"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox7"
        android:layout_gravity="right"
        android:layout_below="@+id/checkBox5"
        android:layout_toRightOf="@+id/checkBox3"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="49dp"
        android:onClick="cb7"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox5"
        android:layout_gravity="right"
        android:layout_marginRight="64dp"
        android:layout_marginBottom="75dp"
        android:layout_alignBottom="@+id/checkBox3"
        android:layout_toLeftOf="@+id/checkBox6"
        android:onClick="cb8"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:id="@+id/checkBox3"
        android:layout_gravity="right"
        android:layout_below="@+id/checkBox4"
        android:layout_alignLeft="@+id/foodpic"
        android:layout_alignStart="@+id/foodpic"
        android:layout_marginLeft="81dp"
        android:layout_marginTop="222dp"
        android:onClick="cb9"/>

        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:id="@+id/realcheckBox4"
            android:layout_alignTop="@+id/checkBox10"
            android:layout_toLeftOf="@+id/checkBox5"
            android:onClick="cb10"/>
    </RelativeLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".15"
            android:layout_marginTop="16dp">
        <Button
            android:id="@+id/yesButton"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/next"
            android:onClick="nextMethodSL12"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"/>
    </LinearLayout>

</LinearLayout>
